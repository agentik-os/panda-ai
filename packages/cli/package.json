{
  "name": "@agentik-os/cli",
  "version": "1.0.0",
  "description": "Agentik OS command-line interface",
  "type": "module",
  "bin": {
    "panda": "./dist/index.js",
    "agentik-os": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "agentik-os",
    "cli",
    "ai",
    "agents"
  ],
  "author": "Agentik OS Team",
  "license": "MIT",
  "dependencies": {
    "@agentik-os/runtime": "workspace:*",
    "@agentik-os/shared": "workspace:*",
    "@inquirer/prompts": "^8.2.0",
    "archiver": "^7.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "form-data": "^4.0.1",
    "inquirer": "^12.3.0",
    "ora": "^8.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
