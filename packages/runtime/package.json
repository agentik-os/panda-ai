{
  "name": "@agentik-os/runtime",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:bench": "vitest bench",
    "test:security": "vitest run tests/security/",
    "test:security:wasm": "vitest run tests/security/wasm-sandbox.security.test.ts",
    "test:security:injection": "vitest run tests/security/injection-attacks.security.test.ts",
    "test:security:permissions": "vitest run tests/security/permission-bypass.security.test.ts",
    "test:security:secrets": "vitest run tests/security/secret-detection.security.test.ts",
    "test:perf": "node tests/performance/load-tests/run-all.js",
    "test:load:agents": "node tests/performance/load-tests/concurrent-agents.js",
    "test:load:throughput": "node tests/performance/load-tests/message-throughput.js",
    "test:stress": "node tests/performance/stress-tests/24h-continuous.js"
  },
  "dependencies": {
    "@agentik-os/shared": "workspace:*",
    "@anthropic-ai/sdk": "^0.74.0",
    "@extism/extism": "2.0.0-rc13",
    "@google-cloud/aiplatform": "^6.4.0",
    "@types/inquirer": "^9.0.7",
    "@types/ws": "^8.18.1",
    "chalk": "^5.3.0",
    "convex": "^1.31.7",
    "cors": "^2.8.6",
    "discord.js": "^14.25.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "inquirer": "^12.3.0",
    "node-cron": "^4.2.1",
    "ollama": "^0.6.3",
    "openai": "^4.76.2",
    "ora": "^8.1.1",
    "telegraf": "^4.16.3",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.16",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.11",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.19.3",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
